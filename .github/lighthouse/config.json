{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "staticDistDir": "./public",
      "url": ["/", "/404", "/politica-de-privacidade"]
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "service-worker": "off",
        "offline-start-url": "off",
        "works-offline": "off"
      }
    }
  }
}
